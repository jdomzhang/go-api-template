{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Project name (no space)",
      "default": "{{destDirName}}"
    },
    "description": {
      "type": "string",
      "required": true,
      "label": "Project description",
      "default": "A go API project"
    },
    "author": {
      "type": "string",
      "label": "Author"
    },
    "license": {
      "type": "string",
      "label": "License",
      "default": "MIT"
    },
    "apiurl": {
      "type": "string",
      "required": true,
      "label": "API URL",
      "default": "api.yourdomain.com"
    },
    "devport": {
      "type": "number",
      "required": true,
      "label": "Dev Server port",
      "default": 9090
    },
    "prodport": {
      "type": "number",
      "required": true,
      "label": "Prod Server port",
      "default": 5001
    },
    "root": {
      "type": "string",
      "required": true,
      "label": "Root account",
      "default": "root@yourdomain.com"
    },
    "gomodule": {
      "type": "string",
      "required": true,
      "label": "Go mod path",
      "default": "github.com/xxx/{{destDirName}}"
    }
  },
  "completeMessage": "{{#inPlace}}To get started:\n\n  yarn dev{{else}}To get started:\n\n  cd {{destDirName}}\n  yarn\n  yarn dev{{/inPlace}}"
}
